RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L]

RewriteRule ^home?$ index.html
RewriteRule ^about?$ about.html
RewriteRule ^blog?$ blog.html
RewriteRule ^blog2?$ blog2.html
RewriteRule ^services?$ services.html
RewriteRule ^contact?$ contact.html
RewriteRule ^projects?$ projects.html
RewriteRule ^certificate?$ certificate.html
RewriteRule ^AC?$ AC.html